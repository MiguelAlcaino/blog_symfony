<html>
    <body>
        <table>
            <thead>
                <th>id</th>
                <th>title</th>
                <th>body</th>
            </thead>
            <tbody>
                {% for post in posts %}
                    <tr>
                        <td>{{ post.id }}</td>
                        <td>{{ post.title }}</td>

                        <td>{{ post.body }}</td>
                    </tr>
                {% endfor %}
            </tbody>
        </table>
    </body>
</html>

